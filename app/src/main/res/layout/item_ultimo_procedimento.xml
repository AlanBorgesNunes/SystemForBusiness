<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="220dp"
    android:layout_height="190dp"
    android:layout_margin="20dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:backgroundTint="@android:color/transparent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_corners_members"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/img_item_ultimos_procedimentos"
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/procedimento"/>
        <TextView
            android:id="@+id/txt_date_item_ultimos_procedimentos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="19-06-2023"
            android:gravity="center"
            android:textSize="16sp"
            android:fontFamily="@font/nunito_regular"
            android:textColor="@color/five"/>
    </LinearLayout>


</androidx.cardview.widget.CardView>